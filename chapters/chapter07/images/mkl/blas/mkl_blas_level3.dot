digraph MKLBLASLevel3 {
    graph [fontname="Microsoft YaHei", fontsize=20, bgcolor="#ffffff", dpi=300, size="10,8", pad=0.5, margin=0.3];
    node [fontname="Microsoft YaHei", fontsize=14, style="filled,rounded"];
    edge [fontname="Microsoft YaHei", penwidth=3, color="#666666"];

    rankdir=TB;
    nodesep=0.8;
    ranksep=1.2;
    splines=spline;
    concentrate=false;
    bgcolor="#ffffff";

    Level3Header [label="BLAS Level 3\n矩阵-矩阵运算\n(Matrix-Matrix Operations)", fillcolor="#c8e6c9", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16];

    BasicMM [label="基本矩阵-矩阵运算\n(Basic Matrix-Matrix)", fillcolor="#e8f5e9", color="#2e7d32", shape=ellipse, style="bold,rounded", fontsize=16, penwidth=2.5];
    GemmOps [label="一般矩阵乘法\n?gemm", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    SymmOps [label="对称矩阵乘法\n?symm", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    TrmmOps [label="三角矩阵乘法\n?trmm", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    TrsmOps [label="三角矩阵求解\n?trsm", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];

    RankUpdates [label="Rank更新运算\n(Rank Updates)", fillcolor="#e8f5e9", color="#2e7d32", shape=ellipse, style="bold,rounded", fontsize=16, penwidth=2.5];
    SyrkOps [label="对称rank-k更新\n?syrk", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    Syr2kOps [label="对称rank-2k更新\n?syr2k", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];

    Hermitian [label="复数厄米矩阵\n(Hermitian Matrix)", fillcolor="#e8f5e9", color="#2e7d32", shape=ellipse, style="bold,rounded", fontsize=16, penwidth=2.5];
    HemmOps [label="厄米矩阵乘法\n?hemm", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    HerkOps [label="厄米rank-k更新\n?herk", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];
    Her2kOps [label="厄米rank-2k更新\n?her2k", fillcolor="#ffffff", color="#2e7d32", shape=box, style="bold,rounded", fontsize=16, penwidth=2.5];


    Level3Header -> BasicMM [penwidth=3.5, arrowsize=1.3, ];
    Level3Header -> RankUpdates [penwidth=3.5, arrowsize=1.3, ];
    Level3Header -> Hermitian [penwidth=3.5, arrowsize=1.3, ];

    BasicMM -> GemmOps [penwidth=3.5, arrowsize=1.3, ];
    BasicMM -> SymmOps [penwidth=3.5, arrowsize=1.3, ];
    BasicMM -> TrmmOps [penwidth=3.5, arrowsize=1.3, ];
    BasicMM -> TrsmOps [penwidth=3.5, arrowsize=1.3, ];

    RankUpdates -> SyrkOps [penwidth=3.5, arrowsize=1.3, ];
    RankUpdates -> Syr2kOps [penwidth=3.5, arrowsize=1.3, ];

    Hermitian -> HemmOps [penwidth=3.5, arrowsize=1.3, ];
    Hermitian -> HerkOps [penwidth=3.5, arrowsize=1.3, ];
    Hermitian -> Her2kOps [penwidth=3.5, arrowsize=1.3, ];
