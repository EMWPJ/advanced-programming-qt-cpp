digraph MKLLAPACKSVD {
    graph [fontname="Microsoft YaHei", fontsize=20, bgcolor="#ffffff", dpi=300, size="10,7",
           pad=0.5, margin=0.3];
    node [fontname="Microsoft YaHei", fontsize=14, style="filled,rounded"];
    edge [fontname="Microsoft YaHei", penwidth=3, color="#666666"];

    rankdir=TB;
    bgcolor="#ffffff";
    nodesep=0.8;
    ranksep=1.2;
    splines=spline;
    concentrate=false;

    SVDModule [label="LAPACK奇异值分解 (SVD)",
               fillcolor="#ffd89b", color="#f57c00",
               shape=box, style="bold,rounded",
               fontsize=22, penwidth=3];

    SVDStandard [label="标准SVD\n?gesvd / ?gesvdq",
                 fillcolor="#fffde7", color="#f57c00",
                 shape=box, style="bold,rounded", fontsize=15, penwidth=2.5];
    SVDDivide [label="分治SVD\n?gesdd / ?gesvdq",
               fillcolor="#fffde7", color="#f57c00",
               shape=box, style="bold,rounded", fontsize=15, penwidth=2.5];
    BidiagSVD [label="双对角SVD\n?bdsvd / ?bdsdc / ?bdsqr",
               fillcolor="#fffde7", color="#f57c00",
               shape=box, style="bold,rounded", fontsize=15, penwidth=2.5];

    SVDModule -> SVDStandard [penwidth=3.5, arrowsize=1.3];
    SVDModule -> SVDDivide [penwidth=3.5, arrowsize=1.3];
    SVDModule -> BidiagSVD [penwidth=3.5, arrowsize=1.3];
}
